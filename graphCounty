#setup general file info
set term png size 1280,1440

set datafile separator ','

#xdata is time with format
set xdata time
set timefmt "%Y-%m-%d"

#pull titles from header column and label x/y axis
set key autotitle columnhead
set xlabel "Date"
set ylabel "Cases"
#rotate x axis for viewing
set xtics rotate
#set y2 axis ticks and label
set y2tics
set y2label 'Deaths'

#move key to top left to stay out of graphs way
set key left top

#graph county
set output outpt
set multiplot layout 3, 1 title titel
set title 'Cases and Deaths (log)'
set logscale y
set logscale y2
plot filename using 1:5 with lines, '' using 1:6 with lines axis x1y2
set title 'New Cases'
unset logscale y
unset logscale y2
set boxwidth 0.9 relative
plot filename using 1:7 with boxes
set title 'New Deaths'
plot filename using 1:8 with boxes
unset multiplot